<html ng-app="twitterApp">

<head>

<meta http-equiv="Cache-Control"
	content="no-cache, no-store, must-revalidate" />
<meta http-equiv="Pragma" content="no-cache" />
<meta http-equiv="Expires" content="0" />

<script
	src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
<link rel="stylesheet"
	href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<link rel="stylesheet"
	href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
<script
	src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js">
	
</script>

<link rel='stylesheet' href='/stylesheets/style.css' />

<title>Twitter</title>

</head>

<body>
	<div class="headerPart">
		<div class="image" style="background-image: url(/images/bg.png)">
			<div style="margin-left: 100px;">
				<div class="birdimage">
					<a href="#"> <img src="/images/checkbird.png"></a>
				</div>
				<div class="buttonsgroup">
					<form class="form-inline">
						<div class="form-group" ng-controller="loginSignupController">
							<input type="submit" value="Sign Up" class="btn btn-sm"
								ng-click="signup()" id="b1">
						</div>
						<div class="form-group" ng-controller="loginController">
							<a type="button" class="btn btn-sm" id="b2" data-toggle="modal"
								data-target="#reserveModal">Log In</a>
						</div>
					</form>
				</div>

				<div>
					<p id="header">See what's happening right now.</p>
					<p id="subheader">Find community, conversation and inspiration
						about the things you love.</p>
				</div>

			</div>
		</div>


		<div style="margin-top: 75px;">
			<img src="/images/check.png" width="100%">
		</div>

		<div class="col-sm-offset-2 col-sm-8" style="margin-top: 20px;">
			<span style="color: darkgray; font-size: 14px;"><span
				style="font-size: 18px;">Moments</span>. View all</span>
		</div>

		<div class="row row-content">
			<div class="col-sm-offset-2 col-sm-7">
				<div class="row">
					<div class="col-sm-3" id="staticbox">
						<div id="innerstaticbox">
							<div class="row">
								<span style="color: darkgray; font-size: 16px;">March
									MADNESS <span style="font-size: 14px;"></span>
								</span>
							</div>
							<div class="row">
								<span style="font-size: 14px;"><b>The internet is
										savage when too...</b></span>
							</div>
							<div class="row">
								<span style="color: darkgray">Earlier today</span>
							</div>
						</div>
						<div class="row">
							<img src="/images/img2.png" width=250px; height=285px;>
						</div>

					</div>
					<div class="col-sm-3" id="staticbox">
						<div id="innerstaticbox">
							<div class="row">
								<span style="color: darkgray; font-size: 16px;">Election
									<span style="font-size: 14px;">2016</span>
								</span>
							</div>
							<div class="row">
								<span style="font-size: 14px;"><b>Is NYC's Dump rally
										the...</b></span>
							</div>
							<div class="row">
								<span style="color: darkgray;">5 hrs ago</span>
							</div>
						</div>
						<div class="row">
							<img src="/images/img1.png" width=250px; height=285px;>
						</div>

					</div>
					<div class="col-sm-3" id="staticbox">
						<div id="innerstaticbox">
							<div class="row">
								<span style="color: darkgray; font-size: 16px;">March
									MADNESS <span style="font-size: 14px;"></span>
								</span>
							</div>
							<div class="row">
								<span style="font-size: 14px;"><b>Kentucky band girl
										is new ...</b></span>
							</div>
							<div class="row">
								<span style="color: darkgray">2 hrs ago</span>
							</div>
						</div>
						<div class="row">
							<img src="/images/img3.png" width=250px; height=285px;>
						</div>

					</div>
				</div>

			</div>
			<div class="col-sm-2" id="loginBox" style="margin-left: -100px;">
				<form class="form-horizontal" role="form"
					ng-controller="loginSignupController">
					<div class="form-group">
						<div class="col-sm-offset-1 col-sm-9">
							<h4>Log in to your account</h4>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-offset-1 col-sm-10" id="loginBoxitem">
							<input type="email" class="form-control" name="email" id="email"
								placeholder="Enter email" ng-model="email" required>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-offset-1 col-sm-10">
							<input type="password" class="form-control" name="password"
								id="password" placeholder="Enter password" ng-model="password"
								required>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-offset-1 col-sm-10" id="rememberMe">
							<label> <input type="checkbox"> Remember me
							</label>
						</div>
					</div>
					<div class="form-group">

						<div class="col-sm-offset-1 col-sm-10">
							<button type="submit" class="btn btn-default col-sm-12">Log
								in</button>
						</div>

					</div>
				</form>
			</div>
		</div>

	</div>

	<div id="reserveModal" class="modal fade" role="dialog"
		ng-controller="loginController">
		<div class="modal-dialog">
			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<p class="modal-title"
						style="text-align: center; color: darkgray; font-size: 14px;">Log
						in to Twitter</p>
				</div>
				<div class="modal-body">
					<form class="form-horizontal">
						<div class="form-group">
							<div class="col-xs-12 col-sm-offset-5 col-sm-5">
								<img src="/images/twitterbird.png" height="20">
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12 col-sm-offset-1 col-sm-10">
								<input type="email" class="form-control" name="email" id="email"
									placeholder="Enter email" ng-model="email" required>
								<div ng-hide="email_error">
									<strong><font color="red">Incorrect Email.
											Please check</font></strong>
								</div>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12 col-sm-offset-1 col-sm-10">
								<input type="password" class="form-control" name="password"
									id="password" placeholder="Enter password" ng-model="password"
									required>
								<div ng-hide="pwd_error">
									<strong><font color="red">Incorrect Password.
											Please check</font></strong>
								</div>
							</div>
						</div>
						<div class="form-group">
							<div class="col-xs-12 col-sm-offset-1 col-sm-10" id="rememberMe">
								<label> <input type="checkbox"> Remember me
								</label>
								<div ng-hide="unexpected_error">
									<strong><font color="red">Unexpected error, try
											again</font></strong>
								</div>
								<div ng-hide="values_error">
									<strong><font color="red">Please enter email
											and password</font></strong>
								</div>
							</div>

						</div>
						<div class="form-group">
							<div class="col-xs-12 col-sm-offset-1 col-sm-10">
								<button type="submit" class="btn btn-default col-sm-12"
									ng-click="login()">Log in</button>
							</div>
						</div>
					</form>

				</div>
				<div class="modal-footer">

					<p class="modal-title"
						style="text-align: center; color: darkgray; font-size: 14px;">
						Don't have an account?<a href="#">Sign up Â»</a>
					</p>
				</div>
			</div>
		</div>
	</div>
</body>
<script src="angularjs/app.js"></script>

</html>